    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrew-chang-dewitt/rvt-crop-modal.git"
  },
  "author": "Andrew Chang-DeWitt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrew-chang-dewitt/rvt-crop-modal/issues"
  },
  "homepage": "https://github.com/andrew-chang-dewitt/rvt-crop-modal#readme",
  "devDependencies": {
    "prettier": "^2.5.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.0"
  },
  "dependencies": {
    "@types/croppie": "^2.6.1",
    "croppie": "^2.6.5",
    "rivet-uits": "^1.8.2"
  }
